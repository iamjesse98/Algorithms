using namespace std;

class Sort{
public:
    int partition(vector<int> &x, int left, int right){
	int pivotIndex=
	swap(x[left],x[left+rand()%(right+1-left)]);
	int 
    }
    void quicksort(vector<int> &x, int left, int right){
	if(left>=right)
	    return;
	int c = partition(x,left,right);
	quicksort(x,left,c-1);
	quicksort(x,c+1,right);
    }
};


